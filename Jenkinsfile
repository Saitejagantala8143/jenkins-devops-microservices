//SCRIPTED PIPELINE (STAGE BLOCKS ARE OPTIONAL)
//DECLARATIVE (all your tasks in STAGES, We cannot have any pipeline without stages )
pipeline {
	agent any
	stages {
		stage('Build') {
			steps {
				echo "Build"	
			}
		}
		stage('Test') {
			steps {	
				echo "Test"	
			}
		}	
		stage('Integration Test') {
			steps {
				echo "Integration Test"		
			}
		}
	}				
 }
		
